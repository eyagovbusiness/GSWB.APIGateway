{
  "HealthEndpoint": "http://localhost:8080/health",
  "Discovery": {
    "Address": "http://gswi.consul:8500"
  },
  "CookieDomain": ".localhost",
  "FrontendURL": "http://localhost:5173",
  "Logging": {
    "Console": {
      "MinimumLevel": "Debug"
    },
    "Graylog": {
      "MinimumLevel": "Debug"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "SwarmBotSwaggerRoute": {
        "ClusterId": "SwarmBotCluster",
        "Match": {
          "Path": "swarm-bot/swagger/v1/swarm-bot/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "MembersSwaggerRoute": {
        "ClusterId": "MembersCluster",
        "Match": {
          "Path": "members-ms/swagger/v1/members-ms/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "EventsSwaggerRoute": {
        "ClusterId": "EventsCluster",
        "Match": {
          "Path": "events-ms/swagger/v1/events-ms/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "SwarmBotCluster": {
        "Destinations": {
          "SwarmBotCluster/destination1": {
            "Address": "http://gswb.swarm-bot:8080/"
          }
        }
      },
      "MembersCluster": {
        "Destinations": {
          "MembersCluster/destination1": {
            "Address": "http://gswb.members:8080/"
          }
        }
      },
      "EventsCluster": {
        "Destinations": {
          "EventsCluster/destination1": {
            "Address": "http://gswb.events:8080/"
          }
        }
      }
    }
  }
}
