{
  "AppName": "APIGateway",
  "HealthEndpoint": "https://apigateway.backend.svc.cluster.local:8080/health",
  "AuthCallbackURI": "/auth-callback",
  "AuthCallbackFailedURI": "/auth-callback-failed",
  "Discovery": {
    "Address": "http://consul-server.backend.svc.cluster.local:8500"
  },
  "Logging": {
    "Console": {
      "Enabled": true,
      "MinimumLevel": "Information"
    },
    "Graylog": {
      "Enabled": false,
      "Host": "localhost",
      "Port": "12201",
      "MinimumLevel": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "SwarmBotRoute": {
        "ClusterId": "SwarmBotCluster",
        "RateLimiterPolicy": "ip-60request/minute-limit",
        "Match": {
          "Path": "swarm-bot/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "MembersRoute": {
        "ClusterId": "MembersCluster",
        "RateLimiterPolicy": "ip-60request/minute-limit",
        "Match": {
          "Path": "members-ms/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "EventsRoute": {
        "ClusterId": "EventsCluster",
        "RateLimiterPolicy": "ip-60request/minute-limit",
        "Match": {
          "Path": "events-ms/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "SwarmBotCluster": {
        "Destinations": {
          "SwarmBotCluster/destination1": {
            "Address": "http://swarmbot.backend.svc.cluster.local:8080/"
          }
        }
      },
      "MembersCluster": {
        "Destinations": {
          "MembersCluster/destination1": {
            "Address": "http://members.backend.svc.cluster.local:8080/"
          }
        }
      },
      "EventsCluster": {
        "Destinations": {
          "EventsCluster/destination1": {
            "Address": "http://events.backend.svc.cluster.local:8080/"
          }
        }
      }
    }
  },
  "Bus": {
    "RabbitMQ": {
      "Publisher": {
        "IntegrationExchange": "api-gateway.exchange"
      },
      "Consumer": {
        "IntegrationQueue": "api-gateway.queue"
      }
    }
  },
  "GuildSwarmDiscordServerId": "1226613544842297417"
}

